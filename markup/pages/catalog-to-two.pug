                                                        span.filter__amount (32)
                                        li.filter__item.filter__item--cost
                                            button(type="button").filter__name.js-filter-show
                                                span.filter__icon Цена
                                            button(type="button").filter__clear
                                            .filter__sublist
                                                .filter__slider
                                                    .filter__fields
                                                        label.filter__price-range
                                                            span от
                                                            input(type="text").filter__cost-field.field.js-from-cost
                                                        label.filter__price-range
                                                            span до
                                                            input(type="text").filter__cost-field.field.js-to-cost
                                                    #slider-range
                                .col-md-8.col-xl-auto.ml-xl-auto
                                    form
                                        ul.filter__list.filter__list--sort
                                            li.filter__item.filter__item--sort
                                                button(type="button").filter__name.js-filter-show
                                                    span.filter__icon Сортировка
                                                ul.filter__sublist
                                                    li.filter__number
                                                        label.filter__checkbox.checkbox
                                                            input(type="radio" name="product-sort")
                                                            span.checkbox__icon
                                                            span.filter__element цена возрастание
                                                    li.filter__number
                                                        label.filter__checkbox.checkbox
                                                            input(type="radio" name="product-sort")
                                                            span.checkbox__icon
                                                            span.filter__element цена убывание
                                                    li.filter__number
                                                        label.filter__checkbox.checkbox
                                                            input(type="radio" name="product-sort")
