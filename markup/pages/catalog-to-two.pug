                                        li.filter__item.filter__item--old
                                            button(type="button").filter__name.js-filter-show
                                                span.filter__icon Возраст
                                            button(type="button").filter__clear
                                            ul.filter__sublist
                                                li.filter__number
                                                    label.filter__checkbox.checkbox
                                                        input(type="checkbox" value="Новорожденные")
                                                        span.checkbox__icon
                                                        span.filter__element Новорожденные
                                                li.filter__number
                                                    label.filter__checkbox.checkbox
                                                        input(type="checkbox" value="от 1 до 3 лет")
