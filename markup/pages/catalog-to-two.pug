                                                        span.filter__element от 1 до 3 лет
                                                li.filter__number
                                                    label.filter__checkbox.checkbox
                                                        input(type="checkbox" value="от 4 до 14 лет")
                                                        span.checkbox__icon
                                                        span.filter__element от 4 до 14 лет
