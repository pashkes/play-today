                                                    li.filter__number
                                                        label.filter__checkbox.checkbox
                                                            input(type="radio" name="product-sort")
                                                            span.checkbox__icon
                                                            span.filter__element сначала скидки
                                                    li.filter__number
                                                        label.filter__checkbox.checkbox
                                                            input(type="radio" name="product-sort")
                                                            span.checkbox__icon
                                                            span.filter__element сначала новые
                                            li.filter__item.filter__item--view-card.filter__item--to-two
                                                button(type="button").filter__name

